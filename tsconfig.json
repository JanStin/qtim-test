{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictBindCallApply": true,
    "strictPropertyInitialization": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "types": [
      "@nuxt/types",
      "@nuxt/image",
      "nuxt"
    ],
    "paths": {
      "@/*": ["./src/*"],
      "~/*": ["./*"],
      "#imports": ["./.nuxt/imports.d.ts"],
      "#app": ["./.nuxt/types/app.d.ts"]
    }
  }
}
